{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".idea": true,
    ".next": true,
    "node_modules": true,
    ".vscode": true
  },
  "explorerExclude.backup": {},
  "cSpell.words": ["shadowsocks", "vless", "vmess"],
  "prisma-smart-formatter.prisma.defaultFormatter": "Prisma.prisma",
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma",
    "editor.formatOnSave": true
  }
}
